@charset "utf-8";
/* CSS Document */

/* =======css reset======= */
body, div, span, object, iframe, h1, h2, h3, h4, h5, h6, p, blockquote, a, big, em, font, img, strong, tt, b, u, i, center, dl, dt, dd, ol, ul, li, fieldset, form, label, table, caption, tbody, tfoot, thead, tr, th, td{ padding: 0; margin: 0; }
table { border-collapse:collapse; border-spacing:0;}
fieldset, img { border:0;}
button,input,select,textarea{ font-size:12px;}
ol,ul,li{ list-style:none}
h1, h2, h3, h4, h5, h6 { font-size:100%;}

/* =======public======= */
/* base */
body{ margin:0 auto; font-size:12px; background:#ffffff; font-family:"微软雅黑","Arial Narrow",HELVETICA;color:#333333;position: relative; }
td { font-size:12px; }
.clear:after { content:"."; display:block; height:0; clear:both; visibility:hidden;}
.clear { display:block; clear:both;}
.hide{ display:none;}
a{ outline:none; text-decoration:none; color:#333333}
a:hover{ color:#333333; text-decoration:none;}

.header{ height: 40px;background: #172b3c;position: fixed; top: 0;width: 100%;z-index: 10;}
.header h1{ color: #ffffff;font-size: 20px;position: absolute;width: 100%;text-align: center;line-height: 40px;}
.header .leftButton img{ height: 25px;float: left;margin: 7.5px 5px;}
.header .rightButton img{ height: 25px;float: right;margin: 7.5px 15px;}
.container{ margin-top: 40px;}
.wrap{ margin: 40px 3% 0;}
.container .btn{ height: 85px;background: #eaedf1;margin-top: -5px;padding-top: 45px;}
.container .btn .submitBtn{ display: block;width: 220px;height: 40px;background: rgba(23,43,60,0.6);color: #ffffff;font-size: 18px;text-align: center;line-height: 40px;margin: 0 auto;}
.container .pic h3{ font-size: 18px;padding: 10px 20px;}
.container .date_wr{ height: 142px;background: #eaedf1; margin-top: -5px;padding-top: 58px;}
.container .date_wr .w_bg{ width: 315px;height: 69px;background: #ffffff;margin: 0 auto;padding-top: 15px;}
.container .date_wr .w_bg p{ color: #f36430;font-size: 18px;text-align: center;}
.wrap > h3{ color: #f36430;font-size: 18px;line-height: 30px;}
.wrap > p{ color: #888888;font-size: 16px;margin-bottom: 10px;}
.wrap .rows_item{ margin-bottom: 15px;}
.wrap .rows_item > p{ color: #888888;font-size: 16px;margin: 0 5%;}
.wrap .rows_item .pic_list{ width: 42%;float: left;}
.wrap .rows_item .pic_right{ width: 58%;float: left;}
.wrap .rows_item .pic_list ul li{ float: left;width: 48%;margin-right: 2%;}
.footer .ft_contact{ background-color: #eaedf1;border-top: 1px solid #cccccc;border-bottom: 1px solid #cccccc;padding: 0px 5% 20px;}
.footer .ft_contact h3{ color: #454545;font-size: 18px;line-height: 40px;}
.footer .ft_contact .tel_wr{ float: left; width: 70%;margin-right: 5%;background-color: #ffffff;height: 35px;padding: 25px 0;}
.footer .ft_contact .QR_wr{ float: left; width: 25%;}
.footer .ft_contact .tel_wr p{ color: #cccccc;}
.footer .ft_contact .tel_wr p span{ color: #f36430;}
.footer .ft_title{ background-color: #eaedf1;border-top: 1px solid #cccccc;margin-top: 20px;height: 50px;}
.footer .ft_title p{ font-size: 16px;color: #f36430;text-align: center;line-height: 50px;}
.footer .ft_title p span{ color: #454545;}
