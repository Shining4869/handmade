<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE urlrewrite
    PUBLIC "-//tuckey.org//DTD UrlRewrite 2.6//EN"
    "http://tuckey.org/res/dtds/urlrewrite2.6.dtd">
<urlrewrite >
  <rule> 
	<note>
		移居网首页
	</note> 
     <from>index/(.*).html</from>  
     <to>index.html?language=$1</to>  
  </rule> 
  <rule>
  	<note>
		新闻详情页
	</note>   
     <from>news/(.*)/(.*)/(.*).html</from>  
     <to>archive/$2.html</to>  
  </rule>
</urlrewrite> 